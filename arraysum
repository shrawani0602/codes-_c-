#include <iostream>
using namespace std;

int main() {
    int n,sum=0;
    cout<<"enter no. of values to be entered in array : ";
    cin>>n;
    int arr[n];
    cout<<"enter values in the array : "<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
       sum=sum+arr[i]; 
    }
    cout<<"sum of array is "<<sum;
    return 0;
}
